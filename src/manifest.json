{
  // Required
  "manifest_version": 2,
  "name": "Vue3 + Typescript",
  "description": "Chrome extension template with Vue 3 + Vuex 4 + Vue Router 4 + Typescript + SCSS + Webpack 5.",
  "version": "0.0.0",
  "icons": {
    "16": "icons/icon-16x16.png",
    "48": "icons/icon-48x48.png",
    "128": "icons/icon-128x128.png"
  },
  // background
  "background": {
    "persistent": false,
    "scripts": [
      "background/index.js"
    ]
  },
  // option
  "options_ui": {
    "page": "options/index.html",
    "open_in_tab": true
  },
  // Popup
  "browser_action": {
    "default_popup": "popup/index.html"
  }
}